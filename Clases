# Clase Administrador

class Administrador:
    Usuario: str
    Contraseña: bool

    def CambiarContraseña(self):
            return "Cambiar Contraseña"

    def EliminarAdministrador(self):
            return "Eliminar Administrador"
            
            
# Clase CarritoCompras

class CarritoCompras:
    Codigo: int

    def __init__(self):
        self.Usuario = Usuario()
        self.Pedido = Pedido()

        def ReiniciarCarritoCompras(self):
            return "Reiniciar Carrito Compras"
        
        def Usuario(self.Usuario):
            return "Usuario"
        
        def Producto(self.Producto):
            return "Producto"

# Clase Cliente

class Cliente:
    Nombre: str
    NumeroTarjeta: int
    Edad: int
    Sexo: str

    def EditarCliente(self):
            return "Editar Cliente"

    def EliminarCliente(self):
            return "Eliminar Cliente"
