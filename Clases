# Clase Administrador

class Administrador:
    Usuario: str
    Contraseña: bool

    def CambiarContraseña(self):
            return "Cambiar Contraseña" 

    def EliminarAdministrador(self):
            return "Eliminar Administrador"
                       
# Clase Cliente

class Cliente:
    Nombre: str
    NumeroTarjeta: int
    Edad: int
    Sexo: str

    def EditarCliente(self):
            return "Editar Cliente"

    def EliminarCliente(self):
            return "Eliminar Cliente"
            
# Clase DetalleFactura

class DetalleFactura:
    ValorUnitario: int
    CantidadProducto: int

    def __init__(self):
        self.Producto = Producto()
        self.Factura = Factura()

        def Producto(self.Producto):
            return "Producto"           
            
# Clase Factura

class Factura:
    NombreEmpresa: str
    Telefono: int
    Correo: bool
    NumeroRUT: int
    NumeroFactura: int
    ValorIVA: int
    
    def __init__(self):
        self.CarritoCompras = CarritoCompras()
        self.DetalleFactura = DetalleFactura ()

        def Producto(self.Producto):
            return "Producto"
        
        def NuevaCuenta(self):
            return "Nueva Cuenta"

        def Descuentos(self):
            return "Descuentos"

        def ValorTotal(self):
            return "Valor Total"

 # Clase Inventario
 
 class Inventario:
    Referencia: str
    Cantidad: int

    def __init__(self):
        self.Producto = Producto ()

        def ActualizarInventario(self):
            return "Actualizar Inventario"

        def ReiniciarInventario(self):
            return "Reiniciar Inventario"
            
 # Clase MetodoPago
 
 class MetodoPago:
    IdPago: int
    TipoPago: str

    def CrearMetodoPago(self):
            return "Crear Usuario"

    def EliminarMetodoPago(self):
            return "Eliminar Metodo Pago"
            
  # Clase Pedido
  
  class Pedido:
    Cantidad: int

    def __init__(self):
        self.Producto = Producto ()
        self.Cliente = Cliente ()

        def ModificarPedido(self):
            return "Modificar Pedido"

        def ReiniciarFactura(self):
            return "Reiniciar Factura"
            
   # Clase Usuario
   
   class Usuario:
    IdUsuario: int
    FechaRegistro: int

    def __init__(self):
        self.DatoContacto = DatoContacto

        def CrearUsuario(self):
            return "Crear Usuario"

        def EliminarUsuario(self):
            return "Eliminar Usuario"
            
   
